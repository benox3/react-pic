{
  "name": "react-pic",
  "version": "0.0.0",
  "description": "A responsive image loading component.",
  "author": "Ben Budnevich",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:umd && npm run build:umd:min",
    "build:umd": "cross-env NODE_ENV=development webpack lib/index.js dist/react-pic.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack lib/index.js dist/react-pic.min.js",
    "clean": "rimraf dist",
    "dev-server": "webpack-dev-server --config webpack.config.dev.js --progress --colors",
    "test": "mocha",
    "lint": "eslint .",
    "start": "cross-env NODE_ENV=development nodemon --exec babel-node playground/server",
    "playground": "npm-run-all -p start dev-server",
    "prebuild": "npm run lint",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benox3/react-pic.git"
  },
  "keywords": [
    "react",
    "pic",
    "picture",
    "image",
    "img",
    "progressive",
    "loading",
    "responsive"
  ],
  "bugs": {
    "url": "https://github.com/benox3/react-pic/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "cross-env": "^1.0.7",
    "eslint": "^2.0.0",
    "eslint-plugin-react": "^4.2.3",
    "express": "^4.14.0",
    "lodash": "^4.16.3",
    "mocha": "^3.0.2",
    "nodemon": "^1.10.2",
    "npm-run-all": "^3.1.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "peerDependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "license": "MIT"
}
